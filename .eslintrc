{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "no-use-before-define": ["error", { "functions": false }]
        "indent": [2, 4],
        "comma-dangle": [2, 'never'],
        "global-require": 0,
        "new-cap": 0,
        "consistent-return": 0
    }
}
